  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/AdventOfCode2022/bin/Debug/net7.0/AdventOfCode2022.dll",
      "args": [
        "1"
      ],
      "cwd": "${workspaceFolder}/AdventOfCode2022",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}